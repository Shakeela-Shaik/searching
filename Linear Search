#include<stdio.h>
int main()
{
   int arr[100],s,c,n;
   printf("enter no. of elements in array\n");
   scanf("%d",&n);
   printf("enter %d integers\n",n);
   for(c=0;c<n;c++)
   {
      scanf("%d",&arr[c]);
   }
   printf("enter no. to search:\n");
   scanf("%d",&s);
   for(c=0;c<n;c++)
   {
       if(arr[c]==s)
       {
           printf("%d is present at location %d",s,c);
           break;
       }
   }
   if(c==n)
   {
       printf("%d is not present in array size",s);
   }
   return 0;
}

